<% render "layouts/errors", :object => @store %>

<%= form_for @store, :url => stores_path, :html => { :multipart => true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>

  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
  <%= f.submit %>
<% end %>
